<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycompany.mapper.PestMapper">
	<insert id="saveImage" parameterType="java.util.Map">
		INSERT INTO img_table VALUES(#{img})
	</insert>
	<select id="login" parameterType="com.mycompany.domain.Member">
		SELECT * FROM members WHERE member_id=#{member_id} and member_pwd=#{member_pwd}
	</select>
	<insert id="join" parameterType="com.mycompany.domain.Member">
		INSERT INTO members(member_id, member_name, member_joindate, admin_yn) values ()
	</insert>
</mapper>